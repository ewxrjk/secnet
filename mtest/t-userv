#! /usr/bin/tclsh

source mtest/common.tcl

#----- success test -----

set good [list Uuser Ginside -u \
          mtest/header.sites $tmp/groupfiles $tmp/out.sites Ginside \
	  < mtest/Ginside.sites]

eval run-mss-userv $good

diff-output e-userv out .sites

